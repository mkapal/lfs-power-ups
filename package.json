{
  "name": "node-insim-example-insim-connection-ts",
  "version": "0.0.0",
  "main": "dist/index.cjs",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/index.tsx",
    "serve:production": "node ./dist/index.cjs",
    "build": "tsup-node src/index.tsx --dts",
    "check-all": "run-s format lint:fix",
    "format": "run-s \"format:no-write --write\"",
    "format:no-write": "prettier \"./**/*.{ts,md,json,yml,yaml}\"",
    "format:check": "run-s \"format:no-write --check\"",
    "lint": "eslint \"./src/**/*\" --ext .js,.ts",
    "lint:fix": "eslint \"./src/**/*\" --ext .js,.ts --fix"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "dotenv": "^16.3.1",
    "node-insim": "3.0.0",
    "react": "^18.2.0",
    "react-node-insim": "file:.yalc/react-node-insim",
    "supports-color": "^9.4.0",
    "tiny-typed-emitter": "^2.1.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.8",
    "@types/node": "^20.4.0",
    "@types/react": "18.2.21",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "prettier": "^3.0.0",
    "tsup": "^7.1.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6"
  },
  "files": [
    "**/*.js",
    "**/*.d.ts"
  ]
}
